                        "type": "object",
                        "properties": {
                          "SSCC": {
                            "type": "string"
                          },
                          "PackageType": {
                            "type": "string"
                          },
                          "PackageLines": {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "properties": {
                                "itemcode": {
